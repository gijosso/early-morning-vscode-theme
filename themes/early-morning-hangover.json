{
  "name": "Early Morning Hangover",
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#282c34",
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "string.comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#495162",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other.object",
        "string constant.other.placeholder",
        "meta.object-literal.key.js"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "PHP Constants",
      "scope": ["constant.other.php"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#b56cc8"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other.template",
        "keyword.other.substitution",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#34b198",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["Keyword", "Storage"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Shadow Punctuation",
      "scope": [
        "punctuation.separator.comma.js",
        "punctuation.separator.parameter.js",
        "punctuation.definition.parameters.begin.js",
        "punctuation.definition.parameters.end.js",
        "punctuation.terminator.statement.js",
        "punctuation.definition.block.js",
        "meta.brace.round.js"
      ],
      "settings": {
        "foreground": "#495162"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded",
        "punctuation.definition.binding-pattern.object.js",
        "meta.object-binding-pattern-variable.js"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#ffdd80"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.logical",
        "keyword.operator.constructor",
        "constant.other.color",
        "storage.type.function.arrow.js",
        "punctuation.definition.typeparameters.begin.js",
        "punctuation.definition.typeparameters.end.js",
        "punctuation.separator.key-value.js",
        "punctuation.accessor.js",
        "punctuation.definition.section.case-statement.js"
      ],
      "settings": {
        "foreground": "#b56cc8",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#ffdd80"
      }
    },
    {
      "name": "Variables constant",
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "name": "Language Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#34b198"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": ["variable.parameter.function.language.special"],
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.template.js",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#72bb6a"
      }
    },
    {
      "name": "Type",
      "scope": ["entity", "entity.name", "support.type"],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Primitive Type",
      "scope": ["support.type.primitive.js", "entity.name.type.js"],
      "settings": {
        "foreground": "#a9b7c6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.class",
        "entity.name.type.class.js",
        "entity.name.module",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#ffdd80"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#ffdd80"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "variable.other.block"],
      "settings": {
        "foreground": "#a9b7c6",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#34b198"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#a9b7c6"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ff5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#b56cc8"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline italic"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff5370"
      }
    },
